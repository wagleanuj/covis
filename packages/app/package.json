{
  "name": "@covis/app",
  "version": "2.1.0",
  "license": "MIT",
  "main": "www/",
  "scripts": {
    "start": "concurrently --names tsc,esb -c green.bold,blue.bold --kill-others \"yarn:bundle --dev\" \"tsc --watch\" ",
    "build": "concurrently --success --names tsc,esb -c green.bold,blue.bold tsc yarn:bundle",
    "bundle": "node ./esbuild.config.mjs",
    "clean": "rimraf www/dist",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.2",
    "@types/node": "14.14.35",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "concurrently": "6.0.1",
    "create-serve": "1.0.1",
    "esbuild": "0.11.5",
    "esbuild-jest": "^0.5.0",
    "identity-obj-proxy": "^3.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.2.3"
  }
}
